    "skipDependencies": true,
    "cacheSourceFiles": false,
    "locDuplicationThreshold": 900000,
    "maxLineLength": 1000,
    "maxTopListSize": 20,
    "analyzerOverrides": [],
    "customHtmlReportHeaderFragment" : "<!-- Global site tag (gtag.js) - Google Analytics -->\n    <script async src=\"https://www.googletagmanager.com/gtag/js?id=UA-129891352-1\"></script>\n  <script>\n    window.dataLayer = window.dataLayer || [];\n\n    function gtag() {\n      dataLayer.push(arguments);\n    }\n\n    gtag('js', new Date());\n    gtag('config', 'UA-129891352-1');\n  </script>"
  },
